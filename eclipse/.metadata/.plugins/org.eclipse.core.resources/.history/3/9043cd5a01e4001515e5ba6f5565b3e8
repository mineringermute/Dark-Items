package com.mineringermute.darkItems.BlocksItems.Blocks;

import com.mineringermute.darkItems.darkItems;
import com.mineringermute.darkItems.BlocksItems.Blocks.material.Block_Dark_Steel;

import cpw.mods.fml.common.registry.GameRegistry;
import net.minecraft.block.Block;
import net.minecraft.block.material.Material;

public final class DarkBlocks {

    public static Block Dark_Steel_Block;

    public static final void init() {
    	GameRegistry.registerBlock(Dark_Steel_Block = new Block_Dark_Steel("Dark_Steel_Block", Material.iron), "Dark_Steel_Block");
    }

    }
